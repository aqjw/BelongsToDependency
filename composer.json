{
    "name": "aqjw/belongs-to-dependency",
    "description": "Extended Laravel Nova BelongsTo field that can depend on other fields",
    "keywords": [
        "laravel",
        "nova",
        "belongs-to"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Aqjw\\BelongsToDependency\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Aqjw\\BelongsToDependency\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "4.0.3"
}